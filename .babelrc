{
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/env", {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }]
      ]
    },
    "esm": {
      "presets": [
        ["@babel/env", {
          "modules": false
        }]
      ]
    },
    "umd": {
      "presets": [
        ["@babel/env", {
          "modules": "commonjs",
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }]
      ]
    },
    "test": {
      "presets": [
        ["@babel/env", {
          "modules": "commonjs"
        }]
      ]
    }
  }
}
